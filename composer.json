{
    "name": "novuso/common-adapter",
    "description": "Common adapters for the Novuso Framework",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "novuso/common": "dev-feature/experimental"
    },
    "require-dev": {
        "aura/di": "3.*",
        "aura/router": "3.*",
        "doctrine/collections": "1.*",
        "doctrine/dbal": "2.6.*",
        "fzaninotto/faker": "1.*",
        "guzzlehttp/guzzle": "6.*",
        "guzzlehttp/promises": "1.*",
        "guzzlehttp/psr7": "1.*",
        "illuminate/container": "5.*",
        "illuminate/routing": "5.*",
        "league/container": "2.*",
        "league/plates": "3.*",
        "mikey179/vfsStream": "1.*",
        "mockery/mockery": "0.9.*",
        "monolog/monolog": "1.*",
        "mustache/mustache": "2.*",
        "pda/pheanstalk": "3.*",
        "php-amqplib/php-amqplib": "2.*",
        "php-di/php-di": "5.*",
        "phpunit/phpunit": "6.*",
        "pimple/pimple": "3.*",
        "predis/predis": "1.*",
        "slim/slim": "3.*",
        "squizlabs/php_codesniffer": "3.*",
        "swiftmailer/swiftmailer": "6.*",
        "symfony/symfony": "3.*",
        "twig/twig": "2.*",
        "willdurand/negotiation": "2.*",
        "zendframework/zend-servicemanager": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Novuso\\Common\\Adapter\\": "src"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
